class Dr => { public Dr() => {} }

class <T> Wrapper => {
    public inner: T;

    public Wrapper(x: T) => {
        self::inner => x;
    }
}


impl Droppable for Dr => {
    public drop() : void => {
        printf("Called drop on Dr !\n");
    }
}

impl <T> NoDrop for Wrapper<T> => {}

let main(): int => {
    let x => @as Wrapper<Dr> @as Dr();
    // Fails at compile time here:
    x::drop();
    return 0;
}
