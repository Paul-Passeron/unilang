@include std::vec
@include std::io

let main(): int => {
    let v => @as Vec<int>();
    printf("Hello, World !\n");
    let i: int => 1;
    while i <= 10 => {
        v::append(i * i + 1);
        i => i + 1;
    }

    printf("Length of v: %d\n", v::len);
    printf("Last value of v: %d\n", v::items[v::len - 1]);
    return 0;
}
