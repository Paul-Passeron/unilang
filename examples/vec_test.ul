@include std::vec
@include std::io
@include std::alloc

class Object => {
    public inner: Vec<i32>;

    public Object() => {
        self::inner => @as Vec<i32>();
    }

    public set(n: i32): void => {
        let i => 0;
        while i < n => {
            self::inner::append(i);
            i => i + 1;
        }
    }

    public get(): i32 => {
        return self::inner::len;
    }
}

impl Droppable for Object => {
    public drop(): void => {
        self::inner::drop();
    }
}

let main(): i32 => {
    let v => @as Vec<i32>();
    let obj => @as Object();
    obj::set(40);
    let res => obj::get();
    printf("obj::get = %d\n", res);
    obj::drop();

    let i: i32 => 1;
    while i <= 54654 => {
        v::append(i);
        i => i + 1;
    }

    printf("Length of v: %d\n", v::len);
    printf("Last value of v: %d\n", v::items[v::len - 1]);
    v::drop();

    let v2 => @as Vec<Object>();
    let i => 1;
    while i < 150 => {
        let obj => @as Object();
        obj::set(i);
        v2::append(obj);
        i => i + 1;
    }

    printf("Length of v2: %d\n", v2::len);
    printf("Last value of v2: %d\n", v2::items[v2::len - 1]::get());

    v2::drop();

    (@as Vec<u8>())::drop();
    (@as Vec<u16>())::drop();
    (@as Vec<u32>())::drop();
    (@as Vec<u64>())::drop();

    (@as Vec<i8>())::drop();
    (@as Vec<i16>())::drop();
    (@as Vec<i32>())::drop();
    (@as Vec<i64>())::drop();

    return 0;
}
