@include std::io

class A => {
    public x: int;
    public A() => { self::x => 8; }
    public normal(): void => {
        printf("Normal method\n");
    }
}

impl A => {
    public method(): int => {
        printf("Hello from impl !\n");
        return self::x + 1;
    }
}

let main(): int => {
    let a => @as A();
    a::method();
    return 0;
}
